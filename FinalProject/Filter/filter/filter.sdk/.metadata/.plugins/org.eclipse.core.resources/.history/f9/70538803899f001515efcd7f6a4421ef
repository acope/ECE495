#include "myGrayscale.h"
#include "Gaussian.h"
#include "xparameters.h"
#include "stdio.h"
//#include "xuartlite.h" //For use of UART, need to figure out how to use!
#include "image.h"
#include "xil_printf.h"
#include "xil_io.h"


/* main function */
int main(void){
	//Variable declaration for Grayscale
	unsigned int PHOTO_HEIGHT,PHOTO_LENGTH = 0;
	u32 Rp, Gp, Bp = 0;
	u16 output;

	//Initial Parameters for Grayscale
	PHOTO_HEIGHT = 100; //Y
	PHOTO_LENGTH = 100; //X
	Rp = 100;
	Gp = 0;
	Bp = 0;
	//End of Parameters for Grayscale

	output = grayscaleFilter(PHOTO_HEIGHT, PHOTO_LENGTH, Rp, Gp, Bp);
	xil_printf("%02X \r\n", output);
	//gaussianFilter();
	return 1;
}
